<%= render partial: "shared/nav_tables" %>
<div class="container">
  <div class="row">
    <h1>美食達人</h1>
    <hr>
      <%  @users.each do |user| %>
        <% if current_user != user %>
        <div class="user_follow col-sm-3 col-md-3 follow_user_item"> 
            <div class="follow_image">
              <%= image_tag("/seed_img/128x128.png") %>
            </div> 
            <div class="user_following_button"> 
              <%= link_to user.name, user_path(user)  %>
              <%= render partial: "shared/follow", locals: {user: user} %>
            </div>
        <% end %>  
      </div>
      <% end %>
  </div>  
</div>  